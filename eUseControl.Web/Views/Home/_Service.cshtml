
@{
    Layout = null;
}

<section id="mu-service">
    <div class="container">
        <div class="row">
            <div class="col-lg-12 col-md-12">
                <div class="mu-service-area">
                    <!-- Start single service -->
                    @{
                        var serObj = Model.ServiceObj;

                        foreach (var serviceO in serObj)
                        {
                            <div class="mu-service-single">
                                <span class="fa fa-book"></span>
                                <h3>@serviceO.Service_title</h3>
                                <p>@serviceO.Service_descript</p>
                            </div>
                        }
                    }
                    <!-- Start single service -->
                </div>
            </div>
        </div>
    </div>
</section>
