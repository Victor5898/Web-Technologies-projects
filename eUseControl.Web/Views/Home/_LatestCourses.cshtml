
@{
    Layout = null;
}

<section id="mu-latest-courses">
    <div class="container">
        <div class="row">
            <div class="col-lg-12 col-md-12">
                <div class="mu-latest-courses-area">
                    <!-- Start Title -->
                    <div class="mu-title">
                        <h2>Ultimele cursuri</h2>
                        <p>Aici sunt prezentate ultimele cursuri incarcate in ultimele 90 de zile</p>
                    </div>
                    <!-- End Title -->
                    <!-- Start latest course content -->
                    @{
                        var cObj = Model.ElemObj;
                        foreach (var co in cObj)
                        {
                            <div class="col-lg-4 col-md-4 col-xs-12">
                                <div class="mu-latest-course-single">
                                    <figure class="mu-latest-course-img">
                                        <a href="#"><img src="@co.LastCourse_images" alt="img"></a>
                                        <figcaption class="mu-latest-course-imgcaption">
                                            <a href="#">@co.LastCourse_name</a>
                                            <span><i class="fa fa-clock-o"></i>@co.LastCourse_days</span>
                                        </figcaption>
                                    </figure>
                                    <div class="mu-latest-course-single-content">
                                        <h4><a href="#">Pentru a accesa cursul online apasa aici</a></h4>
                                        <p>@co.LastCourse_description</p>
                                        <div class="mu-latest-course-single-contbottom">
                                            <a class="mu-course-details" href="#">Detalii</a>
                                            <span class="mu-course-price" href="#">@co.LastCourse_price</span>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        }
                    }
                    <!-- End latest course content -->
                </div>
              </div>
            </div>
        </div>
</section>
